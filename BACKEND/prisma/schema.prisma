
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model ProductTransaction {
  id           Int      @id @default(autoincrement()) 
  product_id   Int      
  title        String
  price        Float
  description  String
  category     String
  image        String
  sold         Boolean
  dateOfSale   DateTime
}
